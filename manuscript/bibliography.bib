	pages = {1387--1402},
	file = {Newmark_McNeally_2018_Impact of habitat fragmentation on the spatial structure of the Eastern Arc.pdf:/Users/khufkens/Dropbox/Research_Files/zotero_library/Newmark_McNeally/Newmark_McNeally_2018_Impact of habitat fragmentation on the spatial structure of the Eastern Arc2.pdf:application/pdf}
