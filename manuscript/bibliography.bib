	author = {Achard, Frédéric and Beuchle, René and Mayaux, Philippe and Stibig, Hans-Jürgen and Bodart, Catherine and Brink, Andreas and Carboni, Silvia and Desclée, Baudouin and Donnay, François and Eva, Hugh D. and Lupi, Andrea and Raši, Rastislav and Seliger, Roman and Simonetti, Dario},
	month = aug,
