	journal = {Journal of Cultural Heritage},
	author = {Nyssen, Jan and Petrie, Gordon and Mohamed, Sultan and Gebremeskel, Gezahegne and Seghers, Valérie and Debever, Martijn and Hadgu, Kiros Meles and Stal, Cornelis and Billi, Paolo and Demaeyer, Philippe and Haile, Mitiku and Frankl, Amaury},
