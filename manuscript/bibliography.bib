	file = {Fauset_et_al_2017_Tropical forest light regimes in a human-modified landscape.pdf:/Users/khufkens/Dropbox/Research_Files/zotero_library/Fauset_et_al/Fauset_et_al_2017_Tropical forest light regimes in a human-modified landscape2.pdf:application/pdf}
