	number = {14},
	urldate = {2019-05-02},
	journal = {Earth Surface Processes and Landforms},
	author = {Mertes, Jordan R. and Gulley, Jason D. and Benn, Douglas I. and Thompson, Sarah S. and Nicholson, Lindsey I.},
	month = nov,
	year = {2017},
