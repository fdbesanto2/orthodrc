	keywords = {Atmosphere, Atmosphere: chemistry, Carbon, Carbon: metabolism, Trees, Trees: metabolism, Trees: growth \& development, Tropical Climate, Biomass, Carbon Dioxide, Carbon Dioxide: analysis, Carbon Dioxide: metabolism, Trees: anatomy \& histology, Trees: chemistry, Biological, Models, Carbon: analysis, Africa, Wilderness, Wood, Wood: analysis, Wood: chemistry},
