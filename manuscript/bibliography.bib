	file = {Mertes_et_al_2017_Using structure-from-motion to create glacier DEMs and orthoimagery from.pdf:/Users/khufkens/Dropbox/Research_Files/zotero_library/Mertes_et_al/Mertes_et_al_2017_Using structure-from-motion to create glacier DEMs and orthoimagery from.pdf:application/pdf}
